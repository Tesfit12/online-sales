{#<h3> {{ ordered_products }}</h3>#}{#        {{ ordered_products.name }}#}{##}{##}{##}{#{% for pro in ordered_products %}#}{#    {{ pro.pic_url }}#}{#{% endfor %}#}{% extends "layout.jin" %}{% block body %}{#=============== the start of slid =============== #}    <div class="card_container container mt-5 ">        <center>            <h2 style="font-family: fantasy"> Your cart </h2>            <h4 style="color: red">                Total :- $ {{ total_price }}            </h4>        </center>        {% if ordered_products is defined and ordered_products|length > 0 %}        {% for item in ordered_products %}                        <div class="col-md-3 card_row">                                <div class="card mb-4 shadow-sm">                                            <div class="solded-container">                                                <span class="product-new-label ml-2">Sale</span>                                                <span class="product-discount-label">20%</span>                                            </div>                                           <div class="item text-center">                                               <img class="card-img-top " src="{{ url_for('static', filename=item.pic_url) }}" alt="Card image" style="width: 130px; height: 170px">                                           </div>                                     <div class="card-body">                                            <div class="product-content">                                                    <h5 class="title"><a href="#">{{ item.name }}</a></h5>                                                <div class="price">${{ item.price }}                                                    <span>${{ item.price * 50/100 }}</span>                                                </div>                                                    <a class="add-to-cart" href="{{ url_for('add_to_cart', product_id=item.id) }}">+ Add To Cart</a>                                           </div>                                     <div class="d-flex justify-content-between align-items-center">                                        <div class="btn-group">                                          <button type="button" class="btn btn-sm btn-warning mr-2 rounded"><a  href="{{ url_for('view', product_id=item.id) }}" >View</a></button>                                          <button type="button" class="btn btn-sm btn-info mr-2 rounded"><a  href="{{ url_for('order', product_id=item.id) }}">order</a></button>                                            <button type="button" class="btn btn-sm btn-danger rounded"><a  href="{{ url_for('delete', product_id=item.id) }}">delete</a></button>                                        </div>                                    <small class="text-muted"> 9 mins </small>                                  </div>                                </div>                              </div>                            </div>    {% endfor %}          {% else %}              <div class="container">                <h6 class="display-4 font-weight-bold">Your cart is empty .... please select items form the product</h6>                <center>                    <a href="/">                    <button class="btn btn-warning my-4 btn-lg">Go Product page</button>                </a>                </center>              </div>            </div>          {% endif %}        </div><center> <div class="row">                <div class="col-md-10">                    <a href="{{ url_for('thank_you') }}">                          <button class="btn btn-lg btn-danger my-3">Buy All</button>                    </a>            </div>        </div>           </center>{% endblock body %}