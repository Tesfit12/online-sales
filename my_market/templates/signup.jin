{% extends "layout.jin" %}{% block body %}<div class="jumbotron">  <div class="container">    <h3 class="display-5 text-center">Sign Up</h3>            <form method="POST" action="" novalidate>                {{  signupform.hidden_tag() }}                <p>                    {{ signupform.username.label }}                    {{ signupform.username(class="form-control form-control-lg", placeholder="Full Name") }}                    <span style="color:red">{{ signupform.username.errors[0] }}</span>                </p>                <p>                    {{ signupform.email.label }}                    {{ signupform.email(class="form-control form-control-lg", placeholder="Email") }}                    <span style="color:red">{{ signupform.email.errors[0] }}</span>                </p>                <p>                    {{ signupform.password.label }}                    {{ signupform.password(class="form-control form-control-lg", placeholder="Password") }}                    <span style="color:red">{{ signupform.password.errors[0] }}</span>                </p>                <p>                    {{ signupform.password_confirm.label }}                    {{ signupform.password_confirm(class="form-control form-control-lg", placeholder="Confirm Password") }}                    <span style="color:red">{{ signupform.password_confirm.errors[0] }}</span>                </p>                <p>                   {{signupform.age.label  }}                    {{ signupform.age(class="form-control form-control-lg", placeholder="Age") }}                    <span style="color:red">{{ signupform.age.errors[0] }}</span>                </p>                <p>                   {{signupform.city.label }}                   {{ signupform.city(class="form-control form-control-lg", placeholder="City") }}                    <span style="color:red">{{ signupform.city.errors[0] }}</span>                </p>                  <div class="form-label-group mb-4">            <button class="btn btn-m btn-success">{{ signupform.submit(class="btn btn-outline") }}</button>        </div>            </form>    </div></div>{% endblock %}