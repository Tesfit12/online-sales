{% extends "layout.jin" %}{% block body %}<div class="jumbotron jumbotron-fluid">  <div class="container">    <h3 class="text-center m-4"> {{ product.name }} </h3>        {#   view   #}      <div class="row">          <div class="col-6 card_row">                <div class="card mb-4 shadow-sm">                            <div class="solded-container m-3">                                <span class="product-new-label ml-3" style="color: limegreen">Sale</span>                                <span class="product-discount-label">20%</span>                            </div>                           <div class="item text-center">                               <img class="card-img-top " src="{{ url_for('static', filename=product.pic_url) }}" alt="Card image" style="width: 200px; height: 200px">                           </div>                     <div class="card-body">                            <div class="product-content">                                    <h3 class="title"><a href="#">{{ product.name}}</a></h3>                                        <div class="price">${{ product.price }}                                            <span>${{ product.price * 50/100 }}</span>                                        </div>                                        <a class="add-to-cart" href="{{ url_for('add_to_cart', product_id=product.id) }}">+ Add To Cart</a>                           </div>                     <div class="d-flex justify-content-between align-items-center">                        <div class="btn-group">{#                          <button type="button" class="btn btn-sm btn-outline-secondary"><a  href="{{ url_for('view', product_id=) }}" >View</a></button>#}                          <button type="button" class="btn btn-sm btn-outline-secondary"><a href="{{ url_for('order', product_id=product.id) }}">order</a></button>                        </div>                    <small class="text-muted">9 mins</small>                  </div>                </div>              </div>        </div>      <div class="col-4">            <p>{{ product.description }}</p><br>            <h4> Availability : {{ product.status }} </h4><br><br>            <h4> Quantity : {{ product.quantity }} </h4><br><br>            <small> date of Sale on : 20/20/2030 </small>      </div>   </div>{# end of row #}</div>{#  end of view #}  </div></div>{% endblock body %}