{% extends "layout.jin" %}{% block body %}<div class="container">    <h4 class="text-center"> Hi, {{ current_user.name[0]|upper}}{{current_user.name[1:] }} here are your orders </h4>    <hr style="border-top: 1px solid red; width: 220px"><table class="table table-dark">  <thead>  <tr>      <th scope="col">#</th>      <th scope="col">Full name</th>      <th scope="col">Email</th>      <th scope="col">Phone</th>      <th scope="col">Address</th>      <th scope="col">Product_id</th>      <th scope="col">Price</th>      <th scope="col"></th>    </tr>  </thead>  <tbody>{#  total_price = sum([o.product_price for o in ordered_obj]) #} {# will give u the total price #}  {% for stuff in ordered_obj %}        <tr>          <th scope="row">{{ loop.index }}</th>          <td>{{ stuff.name}}</td>          <td>{{ stuff.email}}</td>          <td>{{ stuff.phone }}</td>          <td>{{ stuff.address }}</td>          <td>{{ stuff.product_name }}</td>          <td>{{ stuff.product_price }}</td>          <th scope="col">              {# here below the id of product will be               completely changed to   0 1 2 3 4 etc  not like  [H: 1001 ],               so u cant send it to view route, u need to use <stuff.product_name> #}            <a class="cke_button__about_icon" href="{{ url_for('view', product_id=stuff.product_name) }}">Edit</a>            <a class="button text-danger" href="{{ url_for('delete_from_my_order', product_id=stuff.id) }}">Delete</a>          </th>        </tr>  {% endfor %}  </tbody></table><span class="border border-success" style="padding: 11px; color: green"> Total-price: $ {{ total_price }}</span><span class="mr-4">    <button type="button" class="btn btn-success ml-4 p-3"><a href="#">Pay</a></button></span></div>{% endblock %}