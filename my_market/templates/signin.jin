{% extends "layout.jin" %}{% block body %}<div class="jumbotron">  <div class="container">    <h3 class="display-4 text-center">Login</h3>    <form method="POST" action="" novalidate>        {{  signinform.hidden_tag() }}        <div class="form-label-group mb-4">            {{ signinform.username.label(class="form-control-label-lg") }}            {{ signinform.username(class="form-control form-control-lg", placeholder="Username") }}            <span style="color:red">{{ signinform.username.errors[0] }}</span>        </div>        <div class="form-label-group mb-4">            {{ signinform.password.label(class="form-control-label-lg") }}            {{ signinform.password(class="form-control form-control-lg", placeholder="Password") }}            <span style="color:red">{{ signinform.password.errors[0] }}</span>        </div>        <div class="form-label-group mb-4">            <button class="btn btn-m btn-success">{{ signinform.submit(class="btn btn-outline") }}</button>        </div>        <div>        <a href="{{ url_for('reset_request') }}">Forgot Password</a>    </div>          <p class="mt-5 mb-3 text-muted">&copy; 2019-2020</p>    </form>  </div></div>{% endblock %}