{% extends "layout.jin" %}{% block body %}     <h1 class="text-center m-4"> እንቛዕ ብደሓን መጻኹም ናብ ተስፊት ቀጥታዊ ዕድጊ </h1>        <div class="bd-example">{#=============== the starts of slid =============== #}            <div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">                <ol class="carousel-indicators">                  <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>                  <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>                  <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>                </ol>                <div class="carousel-inner">                      {% for item in products.items[:3] %}                           <div class="item {% if loop.index0 == 0 %}active{% endif %}">                               <img style="height: 300px; width: 300px; margin: auto" src="{{ url_for('static', filename=item.pic_url) }}">                           </div>                        {% endfor %}                </div>                  <!-- Left and right controls -->                  <a class="left carousel-control" href="#myCarousel" data-slide="prev">                    <span class="glyphicon glyphicon-chevron-left"></span>                    <span class="sr-only">Previous</span>                  </a>                  <a class="right carousel-control" href="#myCarousel" data-slide="next">                    <span class="glyphicon glyphicon-chevron-right"></span>                    <span class="sr-only">Next</span>                  </a>            </div> {#=============== the end of slid =============== #}        </div>        {#=============== the start of slid ======================================================================== #}    <div class="card_container container mt-5 ">        <h3> Laptops </h3>        <hr style="border-top: 2px dashed limegreen;">        {% for item in products.items %}            {% if  item.category == 'Laptops' %}            <div class="col-md-3 card_row">                <div class="card mb-4 shadow-sm" style="min-height: 290px;">                            <div class="solded-container">                                <span class="product-new-label ml-3" style="color: limegreen">Sale</span>                                <span class="product-discount-label">20%</span>                            </div>                           <div class="item text-center">                               <img class="card-img-top " src="{{ url_for('static', filename=item.pic_url) }}" alt="Card image" style="width: 130px; height: 170px">                           </div>                     <div class="card-body">                            <div class="product-content">                                    <h5 class="title"><a href="{{ url_for('view', product_id=item.id) }}">{{ item.name }}</a></h5>                                <div class="price">${{ item.price }}                                    <span>${{ item.price * 50/100 }}</span>                                </div>                                    <a class="add-to-cart" href="{{ url_for('add_to_cart', product_id=item.id) }}">+ Add To Cart</a>                           </div>                     <div class="d-flex justify-content-between align-items-center">                        <div class="btn-group">                          <button type="button" class="btn btn-sm btn-info mr-2 rounded"><a  href="{{ url_for('view', product_id=item.id) }}" >View</a></button>                          <button type="button" class="btn btn-sm btn-warning mr-2 rounded"><a  href="{{ url_for('order', product_id=item.id) }}">order</a></button>                        </div>                    <small class="text-muted">9 mins</small>                  </div>                </div>              </div>            </div>          {% endif %}        {% endfor %}    </div>{% endblock body %}