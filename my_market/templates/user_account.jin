{% extends "layout.jin" %}{% block body %}    <form method="POST" action="" novalidate>        {{  reset_address.hidden_tag() }}        <p>            {{ reset_address.fullname.label }}            {{ reset_address.fullname() }}            <span style="color:red">{{ reset_address.fullname.errors[0] }}</span>        </p>        <p>            {{ reset_address.email.label }}            {{ reset_address.email() }}            <span style="color:red">{{ reset_address.email.errors[0] }}</span>        </p>        <p>            {{ reset_address.phone.label }}            {{ reset_address.phone() }}            <span style="color:red">{{ reset_address.phone.errors[0] }}</span>        </p>        <p>            {{ reset_address.address.label }}            {{ reset_address.address() }}            <span style="color:red">{{ reset_address.address.errors[0] }}</span>        </p>        <p>            {{ reset_address.submit() }}        </p></form>{% endblock %}